<div class="page-title"><h1><%= @course.name %> &#8212; @ Room <%= @course.room %></h1></div>
<div class="section">
  <div class="section-title"><h3>Course Information</h3></div>
  <div class="section-body">
    <p>Recommended Grade Level: <%= @course.grade %></p>
    <p><%= @course.desc %></p>
  </div>
  <div class="section-title"><h3>Student Instructor Information</h3></div>
  <div class="section-body">
    <p>Student Instructor: <%= @course.teacher %></p>
    <p><%= @course.teacher_desc %></p>
  </div>

  <%= button_to "Register Here!", register_path, class: "btn btn-primary", :method => :get  %>
</div>
